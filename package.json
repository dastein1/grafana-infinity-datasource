    "rest api",
    "scraping",
    "simple-json",
    "url",
    "web scraping",
    "xml"
  ],
  "dependencies": {
    "@grafana/data": "7.2.0",
    "@grafana/runtime": "7.2.0",
    "@grafana/ui": "7.2.0",
    "rxjs": "6.3.3"
  },
  "devDependencies": {
    "@grafana/e2e": "8.1.4",
